{
    "presets": [
        "react",
        "es2015",
        "stage-0"
    ],
    "env": {
        "development": {
            "presets": ["react-hmre"]
        }
    },
    "plugins": [
        "transform-decorators-legacy",
        ["import", {
            "libraryName": "antd",
            "style": true
        }],
        ["transform-runtime", {
            "helpers": false
        }]
    ]
}